---
from: "{{ akash1_address }}"
key: "{{ key }}"
keysecret: "{{ keysecret }}"
domain: "{{ domain }}"
node: "{{ node }}"
withdrawalperiod: "{{ withdrawal_period }}"
chainid: "{{ chain_id }}"
attributes:
  - key: cuda
    value: "{{ cuda_version }}"
  - key: region
    value: "{{ region }}"
  - key: host
    value: "{{ host }}"
  - key: tier
    value: "{{ tier }}"
  - key: organization
    value: "{{ organization }}"
  - key: country
    value: "{{ country }}"
  - key: city
    value: "{{ city }}"
  - key: location-type
    value: "{{ location_type }}"
  - key: capabilities/cpu
    value: "{{ cpu }}"
  - key: capabilities/cpu/arch
    value: "{{ cpu_arch }}"
  - key: capabilities/gpu
    value: "{{ gpu }}"
  - key: capabilities/gpu/vendor/nvidia/model/a100
    value: "{{ gpu_model_a100 }}"
  - key: capabilities/gpu/vendor/nvidia/model/a100/ram/80Gi
    value: {{ gpu_model_a100_ram_80 }}
  - key: capabilities/gpu/vendor/nvidia/model/a100/ram/80Gi/interface/sxm
    value: {{ gpu_model_a100_ram_sxm }}
  - key: capabilities/gpu/vendor/nvidia/model/a100/interface/sxm
    value: {{ gpu_model_a100_sxm }}
  - key: capabilities/storage/1/class
    value: {{ storage_one_class }}
  - key: capabilities/storage/1/persistent
    value: {{ storage_one_persistent }}
  - key: capabilities/storage/2/class
    value: {{ storage_two_class }}
  - key: capabilities/storage/2/persistent
    value: {{ storage_two_persistent }}
  - key: capabilities/memory
    value: {{ memory }}
  - key: network-speed-up
    value: {{ network_speed_up }}
  - key: network-speed-down
    value: {{ network_speed_down }}
email: hosting@ovrclk.com
website: "{{ website }}"
organization: "organization"
price_target_cpu: 1.60
price_target_memory: 0.30
price_target_hd_ephemeral: 0.02
price_target_hd_pers_hdd: 0.01
price_target_hd_pers_ssd: 0.03
price_target_hd_pers_nvme: 0.1
price_target_endpoint: 0.05
price_target_ip: 5
price_target_gpu_mappings: "a100=569,*=569"
